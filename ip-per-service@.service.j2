[Unit]
Description=assigns and waits until all IPs appear on %I
After=network-online.target systemd-networkd-wait-online.service
Wants=network-online.target

[Service]
Type=oneshot
ExecStart={{ script_path }} %I {{ ip_list_prefix_path }}ips-%I.list
RemainAfterExit=true

[Install]
WantedBy=multi-user.target
